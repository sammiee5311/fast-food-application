{
  "title": "Features",
  "type": "object",
  "properties": {
    "distance": {
      "description": "distance from restaurant's location to user's location (km)",
      "minimum": 0,
      "maximum": 10,
      "type": "number"
    },
    "current_time": {
      "description": "current time which is represented by minutes",
      "minimum": 0,
      "maximum": 1440,
      "type": "integer"
    },
    "weather": {
      "description": "an enumeration for weather",
      "enum": [
        "cloudy",
        "sunny",
        "rainny",
        "winndy"
      ],
      "type": "string"
    },
    "traffic": {
      "description": "current traffic ratio",
      "minimum": 1,
      "maximum": 100,
      "type": "integer"
    },
    "season": {
      "description": "an enumeration for season",
      "enum": [
        "spring",
        "summer",
        "fall",
        "winter"
      ],
      "type": "string"
    }
  },
  "required": [
    "distance",
    "current_time",
    "weather",
    "traffic",
    "season"
  ],
  "example": [
    {
      "distance": 3.5,
      "current_time": 245,
      "weather": "sunny",
      "traffic": 34,
      "season": "summer"
    }
  ]
}